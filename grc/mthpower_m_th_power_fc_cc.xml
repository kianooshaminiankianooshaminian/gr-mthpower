<?xml version="1.0"?>
<block>
  <name>m_th_power_fc_cc</name>
  <key>mthpower_m_th_power_fc_cc</key>
  <category>[mthpower]</category>
  <import>import mthpower</import>
  <make>mthpower.m_th_power_fc_cc($M, $N, $symbol_rate)</make>

  <param>
    <name>M (as in M-PSK)</name>
    <key>M</key>
    <type>int</type>
  </param>
  <param>
    <name>Symbol Rate (=Sample Rate in this case)</name>
    <key>symbol_rate</key>
    <type>float</type>
  </param>
  <param>
    <name>N (average frequency estimate)</name>
    <key>N</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>f_est</name>
    <type>float</type>
  </source>
</block>
